<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.nokhba.center.panel.ui.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.nokhba.center.panel.ui.dashboard.DashboardFragment"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_formations"
        android:name="com.nokhba.center.panel.ui.formation.FormationFragment"
        tools:layout="@layout/fragment_services" />

    <fragment
        android:id="@+id/navigation_participants"
        android:name="com.nokhba.center.panel.ui.participants.ParticipantsFragment"
        tools:layout="@layout/fragment_participant" />

    <fragment
        android:id="@+id/navigation_add"
        android:name="com.nokhba.center.panel.ui.add.AddFragment"
        tools:layout="@layout/fragment_add" />

    <fragment
        android:id="@+id/navigation_add_formation"
        android:name="com.nokhba.center.panel.ui.formation.add.AddFormationFragment"
        android:label="@string/title_add_formation"
        tools:layout="@layout/fragment_add_service" />

    <fragment
        android:id="@+id/navigation_add_participant"
        android:name="com.nokhba.center.panel.ui.participants.add.AddParticipant"
        android:label="@string/title_add_participants"
        tools:layout="@layout/add_participant" />
</navigation>